<% include ../partials/_header %>

<div class="container">
  <div class="row">
    <!--FORM-->
    <div class="col-md-5 offset-md-3">
      <div class="card">
        <div class="card-body">
          <form action="/entrenador/editE/<%= Entrenador._id %>" method="post"> 
            <div class="form-group">
              <input class="form-control" type="text" name="NoEntrenador" placeholder="Numero del Entrenador">
            </div>
            <div class="form-group">
              <input class="form-control" type="text" name="Nombre" placeholder="Nombre">
            </div>
            <div class="form-group">
              <input class="form-control" type="text" name="Descripcion" placeholder="Descripcion">
            </div>
            <div class="form-group">
              <input class="form-control" type="text" name="Region" placeholder="Region">
            </div>
            <div class="form-group">
              <input class="form-control" type="text" name="Edad" placeholder="Edad">
            </div>
            <div class="form-group">
            </div>
            </div>
            <button class="btn btn-primary" type="submit">
              Actualizar
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>

<% include ../partials/footer %>
